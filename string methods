var name = 'anurag'
name.toLowerCase() -- coverts to lower case
name.toUpperCase() -- converts to upper case
name.length -- returns length of a string
var num = 9786543210
num.toString().length -- converts to string and retuns length -- chaining operators
name.charAt(number) -- reutnrs character position
var firstName = 'anurag';
var lastName = 'lakavathula';
firstName.charAt(0).toUpperCase()+lastName.charAt(0).toUpperCase() -- AL

 var offers = 'expired at 07.28.2025';
offers.includes('expired') ? 'offer expired' : 'check coupons' -- ternary
'offer expired'
var valid = 'valid unti 10.10.2025';
valid.includes('expired') ? 'offer expired' : 'check coupons'
'check coupons'
